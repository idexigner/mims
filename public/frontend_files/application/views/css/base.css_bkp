/* --------------------------------------------- */
/* advert */
/* --------------------------------------------- */
.advert img{
  position: relative;
  width: 100%;
  height: auto;
  border-radius: 4px;
  overflow: hidden;
  /*margin-top: 30px;*/
}
.in-page-advert {
