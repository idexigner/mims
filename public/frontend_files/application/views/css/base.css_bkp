	width: 100%;
	overflow-x: hidden;
	height: 140px;
}
.ebook-top-slide img,
.ebook-bottom-slide img,
.mims-learning-top-slide img,
.mims-learning-bottom-slide img {
	width: 37%;
	height: 120px;
	margin: 0 auto;
	border-radius: 4px;
	box-shadow: var(--section-box-shadow);
}
@media screen and (max-width: 768px) {
	.ebook-top-slider-container,
	.ebook-bottom-slider-container,
	.mims-learning-top-slider-container,
	.mims-learning-bottom-slider-container {
		margin-top: 10px;
		padding: 10px 0;
		height: 126px;
	}
	.ebook-top-slider-wrapper,
	.ebook-bottom-slider-wrapper,
	.mims-learning-top-slider-wrapper,
	.mims-learning-bottom-slider-wrapper {
		position: relative;
		width: 92vw;
		margin-left: 4vw;
		top: 0;
	}
	.ebook-top-slide img,
	.ebook-bottom-slide img,
	.mims-learning-top-slide img,
	.mims-learning-bottom-slide img {
		width: 40%;
		height: 120px;
		margin: 0 auto;
	}
