.brand-advert-image img {
	align-content: center;
	vertical-align: middle;
	max-width: 260px;
	max-height: 260px;
	width: 100%;
}
/* --------------------------------------------- */
/* content sections */
/* --------------------------------------------- */
.content-section {
  background-color: var(--section-bg);
  padding: 24px;
  margin-top: 30px;
  max-height: 430px;
  border-radius: 4px;
  box-shadow: var(--section-box-shadow);
}
.content-section.main {
  max-height: none;
  height: auto;
  padding: 0;
}
.content-section.min-height {
  padding: 0;
  max-height: none;
  min-height: 470px;
}
.section-header {
  font-family: var(--font-medium);
  color: var(--title-color);
  font-size: var(--font-size-header);
  margin-bottom: 0px;
  padding: 10px;
}
.section-header.drug-update-header {
  margin-bottom: 24px;
}
.section-header.own-pad {
  padding: 30px 30px 10px 30px;
}
.section-header span img {
  height: 32px;
  width: auto;
  margin-right: 10px;
  position: relative;
  top: -1px;
}
.section-header-view-all-btn {
  color: var(--text-color) !important;
  font-size: var(--font-size-regular);
  float: right;
  margin-top: 0px;
  cursor: pointer;
}
.section-header-view-all-btn:hover, .section-header-view-all-btn:active {
  color: var(--theme-color-primary) !important;
}
.section-tab {
  margin-bottom: 0px;
  padding: 0px 30px 24px 30px;
}
.section-tab .nav-tabs .nav-link {
  color: #868686;
  font-size: 16px;
  /* font-weight: 600; */
  font-size: var(--font-size-medium);
  font-family: var(--font-medium);
}
.section-tab .tab-content {
  padding-top: 16px;
}
.section-tab .tab-content .job {
  padding: 10px 0px;
}
.section-tab .nav-tabs .nav-link.active {
  color: #019A3D;
}
.sidebar-tab .side-col-see-more-btn {
  margin-bottom: 0px;
}
.section-job {
  border-radius: 4px;
  background: #fff;
  padding: 32px;
  box-shadow: var(--section-box-shadow);
}
.section-col {
  position: relative;
  padding-bottom: 80px;
}
.section-col .see-more-btn {
  position: absolute;
